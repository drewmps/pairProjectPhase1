<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table>
      <tr>
        <th>email</th>
        <th>action</th>
      </tr>

      <% if (users) { %> <% users.forEach(user => { %>
      <tr>
        <td><%= user.email %></td>
        <td><a href="/delete/<%= user.id %>">Delete</a></td>
      </tr>
      <% }) %> <% } %>
    </table>
  </body>
</html>
